    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.8",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "naive-ui": "^2.12.2",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "typescript": "^4.3.2",
    "vfonts": "^0.1.0",
    "vite": "^2.3.7",
    "vue-eslint-parser": "^7.6.0",
    "vue-tsc": "^0.0.24"
