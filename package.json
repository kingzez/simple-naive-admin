{
  "name": "simple-naive-admin",
  "version": "0.0.0",
  "description": "A Vue3 admin. Uses Vue3, Vite and TypeScript, Not too Slow.",
  "keywords": [
    "Vue3 admin",
    "TypeScript",
    "Vite",
    "Naive admin"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\""
  },
  "author": {
    "name": "Vincent W",
    "email": "vincent.mr.w@gmail.com",
    "url": "https://github.com/kingzez"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingzez/simple-naive-admin.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kingzez/simple-naive-admin/issues"
  },
  "homepage": "https://github.com/kingzez/simple-naive-admin",
  "dependencies": {
    "@vicons/ionicons5": "^0.9.0",
    "date-fns": "^2.22.1",
    "modern-normalize": "^1.1.0",
    "pinia": "^2.0.0-beta.2",
    "vue": "^3.0.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
